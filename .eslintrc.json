{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "operator-linebreak": "off",
    "no-undef": "off",
    "no-console": "off",
    "implicit-arrow-linebreak": "off"
  }
}
